<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>

    <Version>1.0.0.0</Version>
    <IsPackable>true</IsPackable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>MekPathLibrary</PackageId>
    <Title>MekPathLibraryStandard</Title>
    <PackageOutputPath>C:\Nugets</PackageOutputPath>
    <Description>Validate and deal with paths easier</Description>
    <PackageProjectUrl>https://github.com/matikkaeditorinkaantaja/MekUpdater</PackageProjectUrl>
    <Authors>Henri Vainio</Authors>
    <Company>Matikkaeditorink채채nt채j채</Company>
    <PackageIcon>logo v2.1 128x128.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\logo v2.1 128x128.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\logo v2.1 128x128.png">
      <PackagePath>\</PackagePath>
      <Pack>True</Pack>
    </Resource>
  </ItemGroup>

</Project>
